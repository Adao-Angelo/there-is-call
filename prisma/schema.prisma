generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../src/database/db.db"
}

model User {
  id           String  @id @default(cuid())
  name         String
  email        String  @unique
  password     String
  contact      String
  type_account String?
  status       String?
  location     String

  @@map("users")
}

model service {
  id       String @id @default(cuid())
  tittle   String
  agent_id String
  location String
  volume   Float

  @@map("services")
}
